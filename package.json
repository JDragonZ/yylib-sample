  "main": "index.js",
  "scripts": {
    "reg-cn":"npm config set registry https://registry.npm.taobao.org",
    "reg-clear":"npm config delete registry",
    "dev-demo1": "webpack-dev-server --devtool eval --display-error-details --progress --colors --hot --inline --config config.demo1.js ",
    "build-demo1": "set NODE_ENV=prod&&webpack --display-error-details --progress --colors -p --config config.demo1.js",
    "build-src-demo1": "set NODE_ENV=prod&&webpack --display-error-details --progress --colors -d --config config.demo1.js",
    "build-test-demo1": "set NODE_ENV=test&&webpack --display-error-details --progress --colors -p --config config.demo1.js",
    "build-test-src-demo1": "set NODE_ENV=test&&webpack --display-error-details --progress --colors -d --config config.demo1.js",
    "dll-demo1": "webpack --display-error-details --progress --colors -p --config config.demo1.dll.js",
    "dll-src-demo1": "webpack --display-error-details --progress --colors -d --config config.demo1.dll.js"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "bundle-loader": "0.5.4",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "react-hot-loader": "3.0.0-beta.6",
    "style-loader": "0.12.4",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "yylib-react":">=0.72.1"
  }
}
